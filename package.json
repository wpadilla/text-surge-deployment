{
  "name": "text-surge-web-texter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "git push -u https://github.com/wpadilla/text-surge-deployment",
    "generate:svg-sprite": "svg2sprite ./src/assets/icons ./src/assets/sprites/sprite.svg --stripAttrs fill --stripAttrs stroke --stripAttrs id --stripAttrs width --stripAttrs height",
    "dev:ssr": "ng run text-surge-web-texter:serve-ssr",
    "serve:ssr": "node dist/text-surge-web-texter/server/main.js",
    "build:ssr": "ng build && ng run text-surge-web-texter:server",
    "prerender": "ng run text-surge-web-texter:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.3",
    "@angular/cdk": "^12.0.0",
    "@angular/common": "~12.0.3",
    "@angular/compiler": "~12.0.3",
    "@angular/core": "~12.0.3",
    "@angular/forms": "~12.0.3",
    "@angular/platform-browser": "~12.0.3",
    "@angular/platform-browser-dynamic": "~12.0.3",
    "@angular/platform-server": "~12.0.3",
    "@angular/router": "~12.0.3",
    "@ctrl/ngx-emoji-mart": "^5.1.1",
    "@ng-stack/contenteditable": "^1.1.1",
    "@nguniversal/express-engine": "^12.0.1",
    "bootstrap": "^5.0.0",
    "chart.js": "^2.9.4",
    "express": "^4.15.2",
    "primeicons": "^4.1.0",
    "primeng": "^11.4.1",
